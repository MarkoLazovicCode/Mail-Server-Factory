{
  "software": [
    {
      "name": "Hello world",
      "version": "1.0.0",
      "installationSteps": {
        "Docker": [
          {
            "type": "condition",
            "value": "docker --version"
          },
          {
            "type": "conditionCheck",
            "value": "hello-world"
          },
          {
            "type": "deploy",
            "value": "Mocks/Stack:build/Mocks/Stack"
          },
          {
            "type": "stack",
            "value": "build/Mocks/Stack"
          },
          {
            "type": "check",
            "value": "hello-world"
          },
          {
            "type": "command",
            "value": "echo 'Stack test completed with success!'"
          }
        ]
      }
    }
  ]
}